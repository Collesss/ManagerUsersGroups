<Application x:Class="ManagerUsersGroups.WpfUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ManagerUsersGroups.WpfUI"
             xmlns:cv="clr-namespace:ManagerUsersGroups.WpfUI.Converters"
             xmlns:vm="clr-namespace:ManagerUsersGroups.WpfUI.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ApplicationUsersModelView x:Key="ViewModel"/>
            <cv:EnumFlagsConverter x:Key="EnumFlagsAuthenticationTypesConverter" ViewModel="{StaticResource ViewModel}"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\DictionaryCommonStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
